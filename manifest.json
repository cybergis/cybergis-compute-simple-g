{
    "name": "SIMPLE-G",
    "container": "simple-g",
    "execution_stage": "bash /job/executable/run_simpleg.sh",
    "slurm_input_rules": {
        "time": {
            "max": 60,
            "min": 10,
            "default_value": 20,
            "step": 1,
            "unit": "Minutes"
        },
        "memory": {
            "max": 16,
            "min": 8,
            "default_value": 8,
            "step": 1,
            "unit": "GB"
        }
    },
    "require_upload_data": true,
    "supported_hpc": [
        "anvil_community"
    ],
    "default_hpc": "anvil_community"
}