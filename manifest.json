{
    "name": "SIMPLE-G",
    "container": "simple-g",
    "execution_stage": "bash /job/executable/run_simpleg.sh",
    "param_rules": {
        "Model": {
            "type": "string_option",
            "options": [
                "sgmc3_2504",
                "sgw_a2i3c1_20250305"
            ],
            "default_value": "sgmc3_2504"
        }
    },
    "slurm_input_rules": {
        "time": {
            "max": 60,
            "min": 10,
            "default_value": 20,
            "step": 1,
            "unit": "Minutes"
        },
        "memory": {
            "max": 16,
            "min": 8,
            "default_value": 8,
            "step": 1,
            "unit": "GB"
        }
    },
    "supported_hpc": [
        "anvil_community"
    ],
    "default_hpc": "anvil_community"
}
